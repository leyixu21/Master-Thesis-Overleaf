	author = {Vayansky, Ike and Kumar, Sathish A. P.},
	month = dec,
	year = {2020},
	keywords = {Inference algorithms, Probabilistic Bayesian networks, Social Media analysis, Temporal analysis, Text analysis, Topic correlation, Topic modeling, notion},
	pages = {101582},
}

@article{zhou_detecting_2015,
	title = {Detecting tourism destinations using scalable geospatial analysis based on cloud computing platform},
