@article{mohino_identifying_2018,
	title = {Identifying tourist main routes within historic city centres using geotagged photographs},
	abstract = {Geolocated information, in particular that coming from big data and social networks is an undeniable and promising data source for geographical research, especially in the field of tourism mobility. The aim of this paper is twofold. First, to demonstrate the potential of photo-sharing services in the field of tourism mobility. Second, to identify and analyse main tourist attractions and routes at the city level. While some previous studies have already used image-sharing websites to build graphs of the spatial distribution of tourist attractions and routes, this paper contributes first, by connecting these tourists’ graphs/routes with the street layout, and second by focusing on Historic medium-sized cities. In order to achieve the aim, geotagged photographs from Flickr (from January 2010 to December 2017) were explored for the World Heritage City of Toledo (Spain). Those photographs were differentiated according to whether they had been taken by tourists (distinguishing as well between national and international tourists) or local residents.},
